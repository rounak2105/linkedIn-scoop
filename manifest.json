{
    "manifest_version": 3,
    "name": "LinkedIn Email Scraper",
    "version": "1.0",
    "description": "Scrape emails from LinkedIn posts (educational purposes).",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["https://www.linkedin.com/*"],
    "action": {
      "default_popup": "popup.html"
    }
  }
  